#: ../../envvarsbody:1155
msgid "**default:** ``$XDG_CONFIG_HOME/xonsh``"
msgstr ""

#: ../../envvarsbody:1164
msgid "$XONSH_DATA_DIR"
msgstr ""

#: ../../envvarsbody:1165
msgid "This is the location where xonsh data files are stored, such as history."
msgstr ""

#: ../../envvarsbody:1169
msgid "**default:** ``$XDG_DATA_HOME/xonsh``"
msgstr ""

