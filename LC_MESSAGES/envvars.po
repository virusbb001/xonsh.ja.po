msgid ""
"Open desktop standard configuration home dir. This is the same default as"
" used in the standard."
msgstr ""

#: ../../envvarsbody:1041
msgid "**default:** ``~/.config``"
msgstr ""

#: ../../envvarsbody:1050
msgid "$XDG_DATA_HOME"
msgstr ""

#: ../../envvarsbody:1051
msgid ""
"Open desktop standard data home dir. This is the same default as used in "
"the standard."
msgstr ""

#: ../../envvarsbody:1055
msgid "**default:** ``~/.local/share``"
msgstr ""

