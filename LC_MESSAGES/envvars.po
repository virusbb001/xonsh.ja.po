#: ../../envvarsbody:1211
msgid "**default:** ``sys.getdefaultencoding()``"
msgstr ""

#: ../../envvarsbody:1220
msgid "$XONSH_ENCODING_ERRORS"
msgstr ""

#: ../../envvarsbody:1221
