#: ../../envvarsbody:1237
msgid "``XONSH_GITSTATUS_HASH``: ``:``"
msgstr ""

#: ../../envvarsbody:1238
msgid "``XONSH_GITSTATUS_BRANCH``: ``{CYAN}``"
msgstr ""

#: ../../envvarsbody:1239
msgid "``XONSH_GITSTATUS_OPERATION``: ``{CYAN}``"
msgstr ""

#: ../../envvarsbody:1240
msgid "``XONSH_GITSTATUS_STAGED``: ``{RED}●``"
